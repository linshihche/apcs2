#include <iostream>

using namespace std;

int main()
{
    int a, b, product;
    
    printf("A=> ");
    scanf("%d", &a);
    printf("B=> ");
    scanf("%d", &b);
    if(a > b){
        printf("a > b \n");
    }else if (a == b){
        printf("a == b \n");
    }else{
        printf("a < b \n");
    }

    return 0;
}
