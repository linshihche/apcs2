#include <stdio.h>

int main() {
    int a, b, product;
    a = 43;
    b = 6;

 
    // Calculating product
    product = a > b;
    printf("a > b ==> %d\n", product);
    product = a < b;
    printf("a < b ==> %d\n", product);
    product = a == b;
    printf("a == b ==> %d\n", product);
    
    return 0;
}
