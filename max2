#include <iostream>
using namespace std;
int find_max(int a[5],int n){
    int i,max =-10000;
    for(i=0;i<n;i++){
        if(a[i] > max){
            max = a[i];
        }
    }
    return max;
}
int main()
{
    int i;
    int a[5];
    int n;
    scanf("%d",&n);
    for(i=0;i<5;i++){
        scanf("%d", &a[i]);
    }


    printf("the max of a[] = %d",find_max(a,n));

    return 0;
}
